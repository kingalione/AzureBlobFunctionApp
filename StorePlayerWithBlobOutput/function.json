{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "Blob",
      "direction": "out",
      "name": "playerBlob",
      "path": "players/out/stored-input-{id}-{nickName}-{location.country}-{location.region}.json"
    }
  ],
  "scriptFile": "../dist/StorePlayerWithBlobOutput/index.js"
}